{
  "name": "@wundergraph/router-plugin",
  "version": "0.0.1",
  "description": "TypeScript/Bun plugin server library for Cosmo Router",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node --format esm && bun run build:types",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "cosmo",
    "router",
    "plugin",
    "grpc",
    "wundergraph"
  ],
  "author": "WunderGraph",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/wundergraph/cosmo.git",
    "directory": "router-plugin-ts"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.12.4",
    "grpc-health-check": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}

