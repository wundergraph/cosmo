  "author": {
    "name": "WunderGraph Maintainers",
    "email": "info@wundergraph.com"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@grpc/grpc-js": "^1.12.4",
    "grpc-health-check": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "typescript": "5.5.2"
  }
}

