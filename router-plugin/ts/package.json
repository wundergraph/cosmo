{
  "name": "@wundergraph/router-plugin",
  "version": "0.5.0",
  "description": "WunderGraph Cosmo Router Plugin SDK for building plugins with TypeScript",
  "main": "./dist/plugin.js",
  "type": "module",
  "types": "./dist/plugin.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
