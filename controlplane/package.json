{
  "name": "controlplane",
  "version": "0.80.0",
  "private": true,
  "description": "WunderGraph Cosmo Controlplane",
  "type": "module",
  "main": "dist/index.js",
  "author": {
    "name": "WunderGraph Maintainers",
    "email": "info@wundergraph.com"
  },
  "scripts": {
    "dev": "tsc-watch --onSuccess \"node dist/index.js\"",
    "debug": "tsx watch src/index.ts",
    "build": "del dist && tsc",
    "seed": "API_KEY=cosmo_669b576aaadc10ee1ae81d9193425705 tsx src/bin/seed.ts",
    "seed:billing": "tsx src/bin/seed-billing.ts",
    "db:migration-graph": "tsx --no-cache src/bin/create-request-tokens.ts",
    "db:generate": "drizzle-kit generate:pg --config=drizzle.config.ts",
    "db:custom": "drizzle-kit generate:pg --custom --config=drizzle.config.ts",
    "db:check": "drizzle-kit check:pg --config=drizzle.config.ts",
    "db:drop": "drizzle-kit drop --config=drizzle.config.ts",
    "start": "NODE_ENV=production node dist/index.js",
    "test": "pnpm lint && tsc -p tsconfig.test.json && vitest run --reporter=default --reporter=hanging-process",
    "lint": "eslint --cache --ext .ts,.mjs,.cjs . && prettier -c src",
    "lint:fix": "eslint --cache --fix --ext .ts,.mjs,.cjs . && prettier --write -c src",
    "migrate": "pnpm db:migrate && pnpm ch:migrate",
    "ch:down": "dbmate -d \"./clickhouse/migrations\" down",
    "db:migrate": "tsx src/bin/db-migrate.ts",
    "ch:migrate": "tsx src/bin/ch-migrate.ts",
    "ch:new-migration": "dbmate -d \"./clickhouse/migrations\" new",
    "postversion": "name=\"$(node -p \"var a = require('./package.json');process.stdout.write(a.name);process.exit(0)\")\"; version=\"$(node -p \"var a = require('./package.json');process.stdout.write(a.version);process.exit(0)\")\"; gh workflow run image-release.yml -F name=$name -F workingDirectory=controlplane -F tag=$version -F dockerContext=."
  },
  "keywords": [
    "controlplane",
    "wundergraph",
    "cosmo"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.529.1",
    "@bufbuild/buf": "^1.27.2",
    "@connectrpc/connect": "^1.1.3",
    "@connectrpc/connect-fastify": "^1.1.3",
    "@connectrpc/connect-node": "^1.1.3",
    "@fastify/cors": "^9.0.1",
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@graphql-inspector/core": "^5.0.2",
    "@keycloak/keycloak-admin-client": "^22.0.4",
    "@octokit/webhooks-types": "^7.3.1",
    "@tiptap/core": "^2.1.13",
    "@wundergraph/composition": "workspace:*",
    "@wundergraph/cosmo-connect": "workspace:*",
    "@wundergraph/cosmo-shared": "workspace:*",
    "axios": "^1.6.7",
    "axios-retry": "^4.0.0",
    "bullmq": "^5.4.1",
    "cookie": "^0.6.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.29.4",
    "ejs": "^3.1.9",
    "eslint": "^8.57.0",
    "fastify": "^4.26.1",
    "fastify-graceful-shutdown": "^3.5.1",
    "fastify-plugin": "^4.5.1",
    "fastify-raw-body": "^4.3.0",
    "graphql": "^16.7.1",
    "ioredis": "^5.3.2",
    "jose": "^4.15.4",
    "lodash": "^4.17.21",
    "nodemailer": "^6.9.11",
    "nuid": "^1.1.6",
    "octokit": "^3.1.2",
    "openai": "^4.28.4",
    "pino": "^8.19.0",
    "postgres": "^3.4.3",
    "rxjs": "^7.8.1",
    "stream-json": "^1.8.0",
    "stripe": "^14.19.0",
    "tiny-lru": "^11.2.5",
    "uid": "^2.0.2",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.4"
  },
  "devDependencies": {
    "@bufbuild/protobuf": "^1.4.1",
    "@bufbuild/protoc-gen-es": "^1.4.1",
    "@connectrpc/protoc-gen-connect-es": "^1.1.3",
    "@types/cookie": "^0.6.0",
    "@types/ejs": "^3.1.5",
    "@types/eslint": "^8.56.2",
    "@types/lodash": "^4.14.202",
    "@types/node": "^18.19.21",
    "@types/nodemailer": "^6.4.14",
    "@types/prettier": "^3.0.0",
    "@types/stream-json": "^1.7.7",
    "@types/uuid": "^9.0.8",
    "del-cli": "^5.1.0",
    "drizzle-kit": "^0.20.14",
    "eslint-config-unjs": "^0.2.1",
    "eslint-plugin-require-extensions": "^0.1.3",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.2.4",
    "tsc-watch": "^6.0.4",
    "tsx": "^4.7.1",
    "typescript": "^5.2.2",
    "vitest": "^1.4.0"
  }
}
